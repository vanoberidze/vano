#include <iostream>

double min(double x, double y) {
    return (x < y) ? x : y;
}

int main() {
    double num1, num2;

    std::cout << "Enter the first number: ";
    std::cin >> num1;

    std::cout << "Enter the second number: ";
    std::cin >> num2;

    double minValue = min(num1, num2);

    std::cout << "The smaller value is: " << minValue << std::endl;

    return 0;
}
