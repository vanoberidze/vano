#include <iostream>

double harmonicMean(double num1, double num2) {
    return 2.0 / (1.0 / num1 + 1.0 / num2);
}

int main() {
    double num1, num2;

    std::cout << "Enter the first number: ";
    std::cin >> num1;

    std::cout << "Enter the second number: ";
    std::cin >> num2;

    double mean = harmonicMean(num1, num2);

    std::cout << "The harmonic mean is: " << mean << std::endl;

    return 0;
}
